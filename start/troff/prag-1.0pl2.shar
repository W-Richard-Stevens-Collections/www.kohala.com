XBEGIN {
X	MYADDR = "hm@GUUG.de";
X	# number (RE)
X	NUMBER = "^[-+]?([0-9]+[.]?[0-9]*|[.][0-9]+)" "([eE][-+]?[0-9]+)?$";
X	GNAME = "GRAPH";			# label for whole picture
X	FNAME = "FRAME";			# label for frame
X	MNAME = "MARG";			# label for [top|....] margin boxes
X	DEFLSTYLE = "solid";
X	DEFCH = "\\s+2\\(bu\\s-2";
X	MARKED = 1;
X	MINXTICKS = 5;
X	MINYTICKS = 5;
X	nG1 = 0;
X	grapinput = 0;
X	err = 0;
X	dot_lf = 1;
X	warn = 1;
X	debug = 0;
