X## draw line-style
X$1 == "draw" {
X	if (NF != 2) {
X		printf("\"%s\":%d: draw line-style\n", FILENAME, FNR) | "cat >&2";
X		errexit(-1);
X	}
X	if ($2 == "marked") {
X		g_flags[ngs] = MARKED;
X	}
X	else {
X		g_lstyle[ngs] = $2;
X	}
