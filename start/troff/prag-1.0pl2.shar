X		errexit(-1);
X	}
X	next;
X}
X## mark drawing-character
X# set default drawing character for this graph
X$1 == "mark" {
X	if (NF != 2) {
X		printf("\"%s\":%d: mark drawing-character\n", FILENAME, FNR) | "cat >&2";
X		errexit(-1);
