X	++g_ndata[cg];			# count number of data points
X	cg = ngs;				# reset current graph
X	data = 1;
X	next;
X}
X# a single number with opt drawing char
X$1 ~ NUMBER && $2 !~ NUMBER {
X	g_y[cg, g_ndata[cg]] = $1;
X	g_x[cg, g_ndata[cg]] = g_ndata[cg];
X	if (NF >= 2) {
X		$1 = "";
X		sub(/^[ \t]*/, "");
