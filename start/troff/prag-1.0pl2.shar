X## .G1 [width [height]]
X# start of graph input
X/^.G1/ {
X	++nG1;
X	grapinput = 1;
X	data = 0;
X	initgraph();
X	if (NF >= 2) {
X		wid = $2;
X	}
X	if (NF >= 3) {
X		ht = $3;
X	}
X	resizegraph();
X	if (dot_lf) {
X		printf(".lf %d %s\n", FNR, FILENAME);
X	}
X	next;
X}
X## .G2
X# end of graph input -- draw graph
X/^.G2/ {
X	if (!grapinput) {
X		printf("\"%s\":%d: missing previous .G1\n", FILENAME, FNR) |"cat >&2";
X		errexit(-1);
X	}
X	else {
X		endgraph();
X		grapinput = 0;
X	}
X	if (dot_lf) {
X		printf(".lf %d %s\n", FNR, FILENAME);
X	}
X	next;
X}
X# copy all outside from .G1 and .G2
X!grapinput {
X	print $0;
X	next;
